{
  "name": "@factofly/web-app",
  "version": "3.0.5",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev -p 3005",
    "dev:pretty": "pnpm dev | pino-pretty --translateTime -L debug",
    "build": "cross-env ANALYZE=true && next build",
    "build:clean": "rimraf /.next && cross-env NODE_OPTIONS=--max_old_space_size=8192 && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "build:nexus": "npx cross-env NEXUS_BUILD=true ts-node -P ./tsconfig.nexus.json --transpile-only src/server/graphql/schema",
    "tunnel": "lt --port 3005 --subdomain lazy-dragon-1269",
    "graphql:download": "npx apollo service:download --endpoint=http://localhost:3005/api/graphql ./src/graphql/.generated/graphql-schema.json",
    "graphql:generate": "npx apollo codegen:generate --localSchemaFile=./src/graphql/.generated/graphql-schema.json --globalTypesFile=./src/.generated/globalTypes.ts --target=typescript --tagName=gql,graphql .generated",
    "build:graphql": "pnpm build:nexus && pnpm graphql:download && pnpm graphql:generate",
    "postinstall": "npx prisma generate --schema=./prisma/schema.prisma",
    "package:docker": " docker build -t factofly/web-app --env-file .env . ",
    "package": "make-dedicated-lockfile && pnpm package:docker",
    "deploy:dev": "npx dotenv -e .env -- heroku container:push web --app factofly-dev --arg SENTRY_AUTH_TOKEN,BASE_URL,AUTH0_CLIENT_ID,AUTH0_DOMAIN,GOOGLE_TAG_MANAGER_ID,SUPPORT_EMAIL,SENTRY_SAMPLE_RATE,NODE_ENV=development && heroku container:release web --app factofly-dev",
    "deploy:prod": "npx dotenv -e .env.prod -- heroku container:push web --app factofly-prod --arg SENTRY_AUTH_TOKEN,BASE_URL,AUTH0_CLIENT_ID,AUTH0_DOMAIN,GOOGLE_TAG_MANAGER_ID,SUPPORT_EMAIL,SENTRY_SAMPLE_RATE,NODE_ENV=production && heroku container:release web --app factofly-prod"
  },
